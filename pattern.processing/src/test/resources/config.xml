<config>
	<event id="PATH_CHANGE" fileid="apollo">
		<pattern id="pattern">\[STATES_PATH_CHANGE\] source: (\w+), target: (\w+)</pattern>
	</event>
	<event id="STATE_ACTIVATE" fileid="apollo">
		<pattern id="pattern">\[STATE_ACTIVATE\] (\w+)</pattern>
	</event>
		
	<sequence id="STATE_ACTIVATION" name="$(from)->$(var1)">
	   <term index="1" event="PATH_CHANGE">
	       <var name="var1" value="pattern$2"/>
	       <var name="from" value="pattern$1"/>
	   </term>
	   <term index="2" event="STATE_ACTIVATE">
	       <var name="var2" value="pattern$1"/>
	       <condition mvel="var1==var2"/>
	   </term>
	</sequence>
	
	<file id="apollo" path="/home/ppretki/adb/logfileAppolo.log" />
</config>